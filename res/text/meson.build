message_banks_narc_order = files('message_banks.order')

gmm_header_target = meson.current_build_dir() / 'gmm'
gmm_header_includes = include_directories('.')

gmm_header_dir = custom_target('gmm_header_dir',
    capture: true,
    output: 'gmm_header_dir',
    command: [
        'sh', '-c', '"$@"',
        '--',
        'mkdir', '-p', gmm_header_target
    ]
)

gmm_to_bin_gen = generator(msgenc_exe,
    arguments: [
        '-e',
        '--gmm',
        '-c', charmap_txt.full_path(),
        '-H', gmm_header_target / '@BASENAME@' + '.h',
        '@INPUT@',
        '@OUTPUT@'
    ],
    depends: [ charmap_txt, gmm_header_dir ],
    output: '@BASENAME@'
)

message_banks_narc = custom_target('pl_msg.narc',
    output: 'pl_msg.narc',
    input: gmm_to_bin_gen.process(message_bank_files),
    command: [
        knarc_exe,
        '-d', '@PRIVATE_DIR@',
        '-p', '@OUTPUT0@',
        '-o', message_banks_narc_order,
        '-n'
    ]
)

nitrofs_files += message_banks_narc
